= form_for @county do |f|
  -if @county.errors.any?
    #error_explanation
      %h2= "#{pluralize(@county.errors.count, "error")} prohibited this county from being saved:"
      %ul
        - @county.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :County_ID
    = f.text_field :County_ID
  .field
    = f.label :County_Name
    = f.text_field :County_Name
  .field
    = f.label :State_ID
    = f.text_field :State_ID
  .actions
    = f.submit 'Save'
